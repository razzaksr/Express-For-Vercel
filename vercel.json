{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node"
     },
     {
        "src": "app-ui/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir":"build" }
     }
    ],
    "routes": [
     {
      "src": "/api/(.*)","dest": "index.js"
     },
     {
        "src": "/(.*)","dest": "app-ui/build/index.html"
     }
    ]
   }